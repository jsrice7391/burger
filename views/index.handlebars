
<div class="col-md-4 col-md-offset-2">

<ul>
    <h2>Menu</h2>
    {{#each burgers}} 
    {{#unless eaten}} 
    {{> burgers/burger-block eaten=true}} 
    {{/unless}} 
    {{/each}}
</ul>

</div>


<div class="col-md-4 col-md-offset-1">
<ul>
    <h2>Eaten</h2>

    {{#each burgers}} {{#if eaten}} {{> burgers/burger-block eaten=false}} {{/if}} {{/each}}
</ul>
</div>










